	<script>
		var cb = function() {
			var l = document.createElement('link'); l.rel = 'stylesheet';
			l.href = 'css/boots.css';
			var h = document.getElementsByTagName('head')[0]; h.parentNode.insertBefore(l, h);
		};
		var raf = requestAnimationFrame || mozRequestAnimationFrame ||
		webkitRequestAnimationFrame || msRequestAnimationFrame;
		if (raf) raf(cb);
		else window.addEventListener('load', cb);

		var cb2 = function() {
			var m = document.createElement('link'); m.rel = 'stylesheet';
			// m.href = 'https://cloud.typography.com/6664852/762926/css/fonts.css';
			var h = document.getElementsByTagName('head')[0]; h.parentNode.insertBefore(m, h);
		};
		if (raf) raf(cb2);
